FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 3.1e+06;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            fixedValue;
        value           uniform 3.1e+06;
    }
    walls
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 3.1e+06;
    }
    side1
    {
        type            cyclic;
    }
    side2
    {
        type            cyclic;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 3.1e+06;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 3.1e+06;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 3.1e+06;
    }
}


