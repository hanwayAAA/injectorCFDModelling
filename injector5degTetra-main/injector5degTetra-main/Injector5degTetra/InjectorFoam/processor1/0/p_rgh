FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 100000;

boundaryField
{
    Inlet
    {
        type            prghTotalPressure;
        p0              nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    Outlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    Symmetric
    {
        type            symmetry;
    }
    Wall
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 100000;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 100000;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 100000;
    }
}


