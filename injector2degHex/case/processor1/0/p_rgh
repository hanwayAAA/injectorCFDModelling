FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            fixedValue;
        value           uniform 100000;
    }
    walls
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    sides
    {
        type            symmetry;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 100000;
    }
}


